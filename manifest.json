{
  "manifest_version": 3,
  "name": "JS Assignment Automation",
  "description": "JS Assignment Automation Extension for PHero. Created by SANY üçî & Moniruzzaman ‚òïÔ∏è & SAKI üå∂Ô∏è",
  "version": "2.1.1",
  "permissions": ["activeTab", "tabs"],
  "web_accessible_resources": [
    {
      "resources": ["manual-checker.html", "manual-checker.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "sandbox": {
    "pages": ["sandbox.html"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.programming-hero.com/*", "*://*/popup.html"],
      "js": ["root.js", "core.js", "/func/feedback.js", "/func/log1.js", "/func/log2.js"],
      "world": "MAIN"
    }
  ],
  "action": {
    "default_title": "PHero Functional",
    "default_popup": "./popup/popup.html"
  },
  "icons": {
    "16": "./assets/icons8-ok-48.png",
    "48": "./assets/icons8-ok-96.png",
    "128": "./assets/icons8-ok-144.png",
    "256": "./assets/icons8-ok-240.png",
    "512": "./assets/icons8-ok-480.png"
  }
}
